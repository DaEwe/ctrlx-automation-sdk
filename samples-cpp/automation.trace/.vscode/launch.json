    "version": "1.0.0",
    "configurations": [
        {
            "name": "Framework x64",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../../bin/framework/ubuntu20-gcc-x64/rexroth-automation-frame",
            "args": ["${workspaceFolder}/generated/config.properties"],
            "additionalSOLibSearchPath": "${workspaceFolder}/../../bin/framework/ubuntu20-gcc-x64/bin",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set path variable to .",
                    //"text": "set solib-search-path ${workspaceFolder}/generated/ubuntu20-gcc-x64/debug/usr/lib/",
                    "text": "set env LD_LIBRARY_PATH=${workspaceFolder}/../../bin/framework/ubuntu20-gcc-x64/bin",
                    "ignoreFailures": false
                }
            ]
        }
      
    ]
}